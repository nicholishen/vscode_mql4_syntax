{
    "name": "mq4",
    "displayName": "MQL4 Syntax Highlight",
    "description": "MQL4",
    "version": "1.0.4",
    "publisher": "nervtech",
    "author": {
        "name": "Emmannuel Roche",
        "email": "roche.emmanuel@gmail.com",
        "url": "http://nervtech.org"
    },
    "icon": "images/logo.png",
